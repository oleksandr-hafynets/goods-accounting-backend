{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/controllers/**",
          "src/helpers/**",
          "src/middleware/**",
          "src/types/**",
          ".env",
          "DDL.sql"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.ts"
    }
  ]
}
